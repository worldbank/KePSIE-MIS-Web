              <% }else{ %>
              <td></td>
              <% } %>
              <% }else{ %>
              <td></td>
              <% } %>
              <td class="labelCell"><%= __("s"+i) %></td>
              <% if(info["s"+i+"_ms"]==undefined) {%>
              <td class="contentCell1 greyBackground notAvail">N/A</td>
              <% }else {  %>
              <td class="contentCell1 greyBackground"><%=info["s"+i+"_ms"].toFixed(2)%></td>
              <% } %>
              <% if(info["s"+i+"_os"]==undefined) {%>
              <td class="contentCell1 notAvail">N/A</td>
              <% }else { %>
              <td class="contentCell1"><%=info["s"+i+"_os"].toFixed(2)%></td>
              <% } %>
              <% if(info["s"+i+"_ps"]==undefined) {%>
              <td class="contentCell1 notAvail">N/A</td>
              <% }else { %>
              <td class="contentCell1"><%=info["s"+i+"_ps"].toFixed(2)%>%</td>
              <% } %>
            </tr>

            <% } %>
            <tr>
              <!-- <% var total_ms=0;
              var total_os=0;
              var total_os_pre=0;
              
              for(var i=2;i<=13;i++){
