  <% }else if(info["total_os"] < previous_info["total_os"]){  %>
  <td><img src="<%=sails.config.assetURL%>/images/down_16.png" /></td>

  <% }else{ %>
  <td></td>
  <% } %>
  <% }else{ %>
  <td></td>
  <% } %>
  <td class="labelCell boldCell"><%= __("s14d_total") %></td>
  
  <% if(total_ms==undefined) {%>
  <td class="contentCell1 greyBackground boldCell">N/A</td>
  <% }else{ %>
  <td class="contentCell1 greyBackground boldCell">
    <%=total_ms.toFixed(2) %></td>
  <% } %>

  <% if(info["total_os"]==undefined) {%>
  <td class="contentCell1 boldCell">N/A</td>
  <% }else{ %>
    <td class="contentCell1 boldCell"> <%=info["total_os"].toFixed(2) %></td>
  <% } %>

  <% if(info["total_ms"]==undefined || info["total_os"]==undefined) {%>
  <td class="contentCell1 boldCell">N/A</td>
  <% }else{ %>
    <td class="contentCell1 boldCell"><%= ((info["total_os"]*100)/(info["total_ms"])).toFixed(2) %>%</td>
  <% } %>
  </tr>
  <tr class="sectionNote ">
   
    <td colspan="6" class="noteStyle">Note: N/A is used to indicate when information is not available, because health facilities inspected may not have that particular unit/service. </td>
  </tr>
</table>
</td>
</tr>
