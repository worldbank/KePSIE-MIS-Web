
<style>

 

  .actionDiv {
    margin-left: 8px;
    float: left;
  }

</style>
<!-- BEGIN CONTENT -->
<div class="page-content-wrapper" ng-app="auditLogsApp" ng-controller="auditLogsCtrl">
  <!-- BEGIN CONTENT BODY -->
  <div class="page-content">

    <!-- BEGIN PAGE BREADCRUMB -->
    <ul class="page-breadcrumb breadcrumb">
      <li>
        <a href="<%=sails.config.routesPrefix%>/dashboard">Home</a>
        <i class="fa fa-circle"></i>
      </li>
      <li>
        <span class="active">Audit Logs</span>
      </li>
    </ul>
    

    <br>                          

 <div id="responsive" class="modal fade" role="dialog" >
  <div class="modal-dialog">

    <!-- Modal content-->
    <div class="modal-content">
     <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal"
       >&times;</button>
    <h4 class="modal-title bold " style="
    color: darkgray;"> Audit Data </h4>
  </div>
       <div class="modal-body">
       <div class="ng-hide" ng-show="logData">
                                            <div class="row">
                                             <label class="col-md-3 control-label bold">Name :</label>
                                                <div class="col-md-4 ">
                                                <i>
                                                   {{dt.name}}
                                                </i>
                                                </div>
                                                </div>
                                                 <div class="row">
                                                 <label class="col-md-3 control-label bold">Email :</label>
                                                <div class="col-md-4">
                                                   <i>
                                                   {{dt.email}} </i>
                                                </div>
                                                </div>
                                                 <div class="row">
                                                <label class="col-md-3 control-label bold">Contact No :</label>
                                                <div class="col-md-4">
                                                <i>
                                                   {{dt.contactno}} </i>
                                                </div>
                                                </div>
                                                 <div class="row">
                                                 <label class="col-md-3 control-label bold">Organization  :</label>
                                                <div class="col-md-4">
                                                <i>
                                                   {{dt.organization}} </i>
                                                </div>
                                                </div>
                                                 <div class="row">
                                                  <label class="col-md-3 control-label bold">Role :</label>
                                                <div class="col-md-4">
                                                <i>
                                                   {{dt.role}} </i>
                                                </div>
                                                </div>
                                                 <div class="row ng-hide" ng-show="showInspectorId">
                                                 <label class="col-md-3 control-label bold">Inspector ID :</label>
                                                <div class="col-md-4">
                                                <i>
                                                   {{dt.inspectorId}}
                                                   </i>
                                                </div>
                                                </div>
                                                 <div class="row ng-show" ng-hide="reportViewer">
                                                 <label class="col-md-3 control-label bold">County :</label>
                                                <div class="col-md-4">
                                                <i>
                                                   {{dt.county}} </i>
                                                </div>
                                                </div>
                                                </div>
                                                 <div class="ng-hide" ng-show="logData1">
                                                   <div class="row">
                                             <label class="col-md-3 control-label bold">Username :</label>
                                                <div class="col-md-4">
                                                <i>
                                                   {{dt.username}}{{dt.email}}
                                                   </i>
                                                </div>
                                                </div>
                                                 </div>

                                                </div>
                                               
      <div class="modal-footer">
        <button type="button" class="btn btn-default button-rounder" data-dismiss="modal">Close</button>
      </div>
    </div>

  </div>
</div>




    <div class="row" >
      <div class="col-md-12">


        <!-- BEGIN EXAMPLE TABLE PORTLET-->
        <div class="portlet box green" style="background-color: white">
          <div class="headerSection">
            <div class="headerLabelSection">
            Logs
            </div>
            <div class="tools "></div>
          </div>
          <div class="portlet-body">
            <table class="table table-striped table-bordered table-hover" id="userlogs">
              <thead>
                <tr>
                  <th> Sr. No. </th>
                  <th> Name</th>
                  <th> Action</th>
                  <th> Date</th>
                  <th> IP Address</th>
                  
                </tr>
              </thead>

            </table>
          </div>
        </div>
        <!-- END EXAMPLE TABLE PORTLET-->
      </div>
    </div>

    <div id="reportDiv">

    </div>
  </div>
  <!-- END PAGE CONTENT INNER -->
</div>

<script>
  highlightMenu(["auditLogs"]);

</script>


